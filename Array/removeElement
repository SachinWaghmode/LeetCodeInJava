public class Solution {
    public int removeElement(int[] nums, int val) {
        int i=0;
        int size= nums.length;
        int j=0;
        while(i<size)
        {
            
            if(nums[i]!= val)
            {
                
                nums[j]= nums[i];
                j++;
            }
            i++;
           
        }
        return j;
        
    }
}
